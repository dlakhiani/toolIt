{
    "name": "toolit",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build && npx tsc --outDir build server/server.ts",
        "preview": "vite preview",
        "start": "npx nodemon server/server.ts",
        "start:tsx": "npx tsx watch server/server.ts",
        "start:new": "ts-node --esm server/server.ts",
        "lint": "npx eslint . --ignore-pattern \"env.d.ts\"",
        "build:css": "./node_modules/.bin/tailwindcss -i ./src/assets/main.css -o ./src/assets/output.css"
    },
    "dependencies": {
        "@emailjs/browser": "^4.4.1",
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/api-logs": "^0.57.0",
        "@opentelemetry/auto-instrumentations-node": "^0.55.0",
        "@opentelemetry/exporter-metrics-otlp-http": "^0.57.0",
        "@opentelemetry/exporter-metrics-otlp-proto": "^0.57.0",
        "@opentelemetry/exporter-trace-otlp-http": "^0.57.0",
        "@opentelemetry/exporter-trace-otlp-proto": "^0.57.0",
        "@opentelemetry/sdk-logs": "^0.57.0",
        "@opentelemetry/sdk-metrics": "^1.30.0",
        "@opentelemetry/sdk-node": "^0.57.0",
        "axios": "^1.4.0",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.2",
        "express-session": "^1.18.1",
        "mongoose": "^8.9.2",
        "openai": "^4.77.0",
        "pinia": "^2.3.1",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.19.3",
        "vite": "^6.1.0",
        "vue": "^3.3.4",
        "vue-router": "^4.2.4"
    },
    "devDependencies": {
        "@eslint/js": "^9.17.0",
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/node": "^20.4.5",
        "@vitejs/plugin-vue": "^5.2.1",
        "eslint": "^9.17.0",
        "nodemon": "^3.0.1",
        "prettier": "^3.4.2",
        "prettier-eslint": "^16.3.0",
        "tailwindcss": "^4.0.13",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.18.2",
        "vue-eslint-parser": "^8.3.0",
        "vue-tsc": "^1.8.5"
    }
